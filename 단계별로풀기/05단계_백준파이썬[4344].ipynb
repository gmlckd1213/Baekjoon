   "source": [
    "t=int(input())\n",
    "for i in range(t):\n",
    "    score_list=list(map(int,input().split()))\n",
    "    avg=sum(score_list[1:])/score_list[0]\n",
    "    count=0\n",
    "    for t in score_list[1:]:\n",
    "        if(t>avg):\n",
    "            count+=1\n",
    "    per=count/score_list[0]*100\n",
    "    print(\"%.3f%%\" %per)"
   ]
  }
